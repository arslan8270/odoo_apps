<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Assets-->
    <template id="assets_frontend_testimonial" name="Frontend assets" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="after">
          <link href="/website_testimonial_snippet_pack2/static/src/owl_carousel/assets/owl.carousel.min.css"
            rel="stylesheet" type="text/css"/>
          <link href="/website_testimonial_snippet_pack2/static/src/owl_carousel/assets/owl.theme.default.min.css"
            rel="stylesheet" type="text/css"/>
          <link href="/website_testimonial_snippet_pack2/static/src/css/animate.min.css"
            rel="stylesheet" type="text/css"/>
          <link href="/website_testimonial_snippet_pack2/static/src/less/backend.less"
              rel="stylesheet" type="text/css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
          <script type="text/javascript"
            src="/website_testimonial_snippet_pack2/static/src/owl_carousel/owl.carousel.min.js"/>
          <script type="text/javascript"
            src="/website_testimonial_snippet_pack2/static/src/js/backend.js"/>
          <script type="text/javascript"
            src="/website_testimonial_snippet_pack2/static/src/js/frontend.js"/>
        </xpath>
    </template>
</odoo>
